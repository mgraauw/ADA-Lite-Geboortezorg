<?xml version="1.0" encoding="UTF-8"?>
<ada-viewer-template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/viewer-template.xsd"
  specification="kernset-22-definition.xml" >

  <title>{#} - BSN {vrouw/burgerservicenummer}</title>

  <header context="vrouw">
   
    <auto-items/>
  </header>

  <section color="red">
 
    <table-view context="zorgverlening">  
      <item context="datum_start_zorgverantwoordelijkheid"/>
      <item context="datum_einde_zorgverantwoordelijkheid"/>
      <item context="eindverantwoordelijk_in_welke_perinatale_periodeq">
        <prompt>Perinatale periode</prompt>
      </item>
      <item context="conclusie_risicostatus_na_intake">
        <prompt>Risicostatus</prompt>
      </item>
    </table-view>

  </section>
  
  <section color="blue" context="zwangerschap">
    <title>Zwangerschap</title>
    
    <table-view>
      <item context="graviditeit"/>
      <item context="pariteit_voor_deze_zwangerschap">
        <prompt>Pariteit</prompt>
      </item>
      <item>
        <prompt>A terme datum</prompt>
        <value>{definitieve_a_terme_datum}</value>
      </item>
      <item>
        <prompt>Voorgenomen lokatie</prompt>
        <value>{voornemens/voorgenomen_plaats_baring_tijdens_zwangerschap_type_locatie}</value>
      </item>
    </table-view>
  </section>
  
  <section color="yellow" context="zwangerschap/prenatale_controle">
    
    <table-view>
      <auto-items/>
    </table-view>
  </section>

</ada-viewer-template>
