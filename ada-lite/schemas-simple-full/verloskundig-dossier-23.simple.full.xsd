               <xs:documentation source="desc" xml:lang="nl-NL">Maximaal aantal foetus geobserveerd in de betreffende zwangerschap.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="amnioniciteit"
                     minOccurs="0"
                     maxOccurs="1"
