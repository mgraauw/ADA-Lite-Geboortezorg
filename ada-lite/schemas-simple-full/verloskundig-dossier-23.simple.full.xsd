               <xs:documentation source="desc" xml:lang="nl-NL">Eén of meer foetus in het chorion (meerdere foetus in één chorion is monochoriaal).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="consanguiniteitq"
                     minOccurs="0"
                     maxOccurs="1"
                     type="consanguiniteitq">
